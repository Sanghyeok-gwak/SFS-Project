-----------------해당 계정 내에 존재하는 DB 객체들 삭제하는 PLSQL문------------------
-- 해당 계정의 모든 트리거 삭제
BEGIN
FOR C IN (SELECT * FROM USER_TRIGGERS) LOOP
  EXECUTE IMMEDIATE 'DROP TRIGGER '||C.TRIGGER_NAME;
END LOOP;
END;
/
--해당 계정의 모든테이블 및 제약조건 삭제
BEGIN
    FOR C IN (SELECT * FROM USER_TABLES) LOOP
    EXECUTE IMMEDIATE ('DROP TABLE "'||C.TABLE_NAME||'" CASCADE CONSTRAINTS');
    END LOOP;
END;
/
--해당 계정의 모든 시퀀스 삭제
BEGIN
FOR C IN (SELECT * FROM USER_SEQUENCES) LOOP
  EXECUTE IMMEDIATE 'DROP SEQUENCE '||C.SEQUENCE_NAME;
END LOOP;
END;
/
--해당 계정의 모든 뷰 삭제
BEGIN
FOR C IN (SELECT * FROM USER_VIEWS) LOOP
  EXECUTE IMMEDIATE 'DROP VIEW '||C.VIEW_NAME;
END LOOP;
END;
/
------------------------------------------------------------------

create TABLE "MEMBER" (
	"MEM_NO"	VARCHAR2(50)		NOT NULL,
	"MEM_PWD"	CLOB		NULL,
	"MEM_NAME"	VARCHAR2(50)		NULL,
	"MEM_PHONE"	VARCHAR2(50)		NULL,
	"MEM_EMAIL"	VARCHAR2(100)		NULL,
	"MEM_ADDR"	VARCHAR2(300)		NULL,
	"HIRE_DATE"	DATE		NULL,
	"END_DATE"	DATE		NULL,
	"STATUS"	CHAR(3)	DEFAULT 'Y'	NULL,
	"MEM_SSN"	VARCHAR2(50)		NULL,
	"ACCOUNT"	VARCHAR2(100)		NULL,
	"PROFILE_URL"	VARCHAR2(500)		NULL,
	"DEPT_CODE"	VARCHAR2(10)		NULL,
	"POS_CODE"	VARCHAR2(10)		NULL
);

COMMENT ON COLUMN "MEMBER"."MEM_NO" IS '사원번호';

COMMENT ON COLUMN "MEMBER"."MEM_PWD" IS '사원비밀번호';

COMMENT ON COLUMN "MEMBER"."MEM_NAME" IS '사원이름';

COMMENT ON COLUMN "MEMBER"."MEM_PHONE" IS '사원전화번호';

COMMENT ON COLUMN "MEMBER"."MEM_EMAIL" IS '사원이메일';

COMMENT ON COLUMN "MEMBER"."MEM_ADDR" IS '사원주소';

COMMENT ON COLUMN "MEMBER"."HIRE_DATE" IS '사원입사일';

COMMENT ON COLUMN "MEMBER"."END_DATE" IS '사원퇴사일';

COMMENT ON COLUMN "MEMBER"."STATUS" IS 'Y:근무 N:퇴사';

COMMENT ON COLUMN "MEMBER"."MEM_SSN" IS '사원주민번호';

COMMENT ON COLUMN "MEMBER"."ACCOUNT" IS '사원계좌번호';

COMMENT ON COLUMN "MEMBER"."PROFILE_URL" IS '사원프로필사진';

COMMENT ON COLUMN "MEMBER"."DEPT_CODE" IS '직급코드';

COMMENT ON COLUMN "MEMBER"."POS_CODE" IS '부서코드';

CREATE TABLE "ATTACHMENT" (
	"FILE_NO"	NUMBER		NOT NULL,
	"FILE_PATH"	VARCHAR2(200)		NULL,
	"FILE_SYSTEM_NAME"	VARCHAR2(500)		NULL,
	"FILE_ORIGINAL_NAME"	VARCHAR2(500)		NULL,
	"FILE_UPLOAD_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"FILE_REF_TYPE"	VARCHAR2(20)		NULL,
	"FILE_REF_NO"	NUMBER		NULL
);

COMMENT ON COLUMN "ATTACHMENT"."FILE_NO" IS '파일번호';

COMMENT ON COLUMN "ATTACHMENT"."FILE_PATH" IS '파일경로';

COMMENT ON COLUMN "ATTACHMENT"."FILE_SYSTEM_NAME" IS '파일시스템이름';

COMMENT ON COLUMN "ATTACHMENT"."FILE_ORIGINAL_NAME" IS '파일원본명';

COMMENT ON COLUMN "ATTACHMENT"."FILE_UPLOAD_DATE" IS '업로드날짜';

COMMENT ON COLUMN "ATTACHMENT"."FILE_REF_TYPE" IS '게시물종류';

COMMENT ON COLUMN "ATTACHMENT"."FILE_REF_NO" IS '참조번호';

CREATE TABLE "SALARY" (
	"SAL_SALARY"	NUMBER		NULL,
	"SAL_PENSION"	NUMBER		NULL,
	"SAL_HEALTH"	NUMBER		NULL,
	"SAL_EMPLOYMENT"	NUMBER		NULL,
	"SAL_CARE"	NUMBER		NULL,
	"SAL_PAYMENT_DATE"	DATE		NULL,
	"SAL_MONTH"	DATE		NULL,
	"SAL_MEM_NO"	VARCHAR2(50)		NULL
);

COMMENT ON COLUMN "SALARY"."SAL_SALARY" IS '기본급';

COMMENT ON COLUMN "SALARY"."SAL_PENSION" IS '국민연금';

COMMENT ON COLUMN "SALARY"."SAL_HEALTH" IS '건강보험';

COMMENT ON COLUMN "SALARY"."SAL_EMPLOYMENT" IS '고용보험';

COMMENT ON COLUMN "SALARY"."SAL_CARE" IS '장기요양';

COMMENT ON COLUMN "SALARY"."SAL_PAYMENT_DATE" IS '지급날짜';

COMMENT ON COLUMN "SALARY"."SAL_MONTH" IS '급여날짜';

COMMENT ON COLUMN "SALARY"."SAL_MEM_NO" IS '사원번호';

CREATE TABLE "CHAT" (
	"CHAT_NO"	NUMBER		NOT NULL,
	"CHAT_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"CHAT_NEWMSG"	VARCHAR2(1000)		NULL,
	"MSG_NO"	NUMBER		NULL,
	"MEM_NO"	VARCHAR2(50)		NULL
);

COMMENT ON COLUMN "CHAT"."CHAT_NO" IS '채팅방번호';

COMMENT ON COLUMN "CHAT"."CHAT_DATE" IS '생성일자';

COMMENT ON COLUMN "CHAT"."CHAT_NEWMSG" IS '최근메세지';

COMMENT ON COLUMN "CHAT"."MSG_NO" IS '메세지번호';

COMMENT ON COLUMN "CHAT"."MEM_NO" IS '생성유저';

CREATE TABLE "ALERT" (
	"ALERT_NO"	VARCHAR(255)		NOT NULL
);

CREATE TABLE "MESSAGE" (
	"MSG_NO"	NUMBER		NOT NULL,
	"MSG_CONTENT"	VARCHAR2(2000)		NOT NULL,
	"MSG_REGIST_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"MSG_STATUS"	VARCHAR2(5)	DEFAULT 'Y'	NOT NULL,
	"CHAT_NO"	NUMBER		NULL,
	"MEM_NO"	VARCHAR2(50)		NULL
);

COMMENT ON COLUMN "MESSAGE"."MSG_NO" IS '메세지번호';

COMMENT ON COLUMN "MESSAGE"."MSG_CONTENT" IS '내용';

COMMENT ON COLUMN "MESSAGE"."MSG_REGIST_DATE" IS '보낸날짜';

COMMENT ON COLUMN "MESSAGE"."MSG_STATUS" IS '메세지상태';

COMMENT ON COLUMN "MESSAGE"."CHAT_NO" IS '채팅방번호';

COMMENT ON COLUMN "MESSAGE"."MEM_NO" IS '사원번호';

CREATE TABLE "COMMON_CODE" (
	"CD"	VARCHAR2(300)		NULL,
	"NM"	VARCHAR2(300)		NULL,
	"CREATER"	NUMBER		NULL,
	"MODIFY"	NUMBER		NULL,
	"CREATE_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"MODIFY_DATE"	DATE	DEFAULT SYSDATE	NULL
);

COMMENT ON COLUMN "COMMON_CODE"."CD" IS '코드';

COMMENT ON COLUMN "COMMON_CODE"."NM" IS '코드명';

COMMENT ON COLUMN "COMMON_CODE"."CREATER" IS '생성자';

COMMENT ON COLUMN "COMMON_CODE"."MODIFY" IS '수정자';

COMMENT ON COLUMN "COMMON_CODE"."CREATE_DATE" IS '생성일자';

COMMENT ON COLUMN "COMMON_CODE"."MODIFY_DATE" IS '수정일자';

CREATE TABLE "EDOC" (
	"EDOC_NO"	NUMBER		NOT NULL,
	"EMP_NO"	NUMBER		NOT NULL,
	"EDOC_DOT_TYPE"	VARCHAR2(100)		NULL,
	"EDOC_TITLE"	VARCHAR2(300)		NULL,
	"EDOC_CONTENT"	CLOB		NULL,
	"EDOC_STARTDATE"	DATE	DEFAULT SYSDATE	NULL,
	"EDOC_ENDDATE"	DATE		NULL,
	"FINAL_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"EDOC_STATUS"	VARCHAR2(30)		NULL,
	"CREATER"	NUMBER		NULL,
	"MODIFIER"	NUMBER		NULL,
	"MODIFY_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"USE_YN"	CHAR(1)	DEFAULT 'Y'	NULL
);

COMMENT ON COLUMN "EDOC"."EDOC_NO" IS '전자문서번호';

COMMENT ON COLUMN "EDOC"."EDOC_DOT_TYPE" IS '양식유형';

COMMENT ON COLUMN "EDOC"."EDOC_TITLE" IS '제목';

COMMENT ON COLUMN "EDOC"."EDOC_CONTENT" IS '문서내용';

COMMENT ON COLUMN "EDOC"."EDOC_STARTDATE" IS '시작일시(생성일)';

COMMENT ON COLUMN "EDOC"."EDOC_ENDDATE" IS '종료일시(만료일시)';

COMMENT ON COLUMN "EDOC"."FINAL_DATE" IS '완료일';

COMMENT ON COLUMN "EDOC"."EDOC_STATUS" IS 'T(임시저장),S(결제대기),I(결재진행중),F(결재완료),R(결재반려)';

COMMENT ON COLUMN "EDOC"."CREATER" IS '기안자(생성자,사원번호)';

COMMENT ON COLUMN "EDOC"."MODIFIER" IS '수정자(사원번호)';

COMMENT ON COLUMN "EDOC"."MODIFY_DATE" IS '수정일';

COMMENT ON COLUMN "EDOC"."USE_YN" IS '사용유무(Y/N)';

CREATE TABLE "DEPARTMENT" (
	"DEPT_CODE"	VARCHAR2(300)		NULL
);

COMMENT ON COLUMN "DEPARTMENT"."DEPT_CODE" IS '부서코드';

CREATE TABLE "POSITION" (
	"POS_CODE"	VARCHAR2(300)		NULL,
	"POS_LEVEL"	NUMBER		NULL
);

COMMENT ON COLUMN "POSITION"."POS_CODE" IS '직급코드';

COMMENT ON COLUMN "POSITION"."POS_LEVEL" IS '직급레벨';

CREATE TABLE "EDOC_SAMPLE" (
	"SAMPLE_NO"	NUMBER		NOT NULL,
	"SAMPLE_DOT_CODE"	VARCHAR2(100)		NULL,
	"SAMPLE_DESC"	VARCHAR2(400)		NULL,
	"SAMPLE_FORMAT"	CLOB		NULL,
	"EMP_NO"	NUMBER		NULL,
	"CREATE_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"MODIFIER"	NUMBER		NULL,
	"MODIFT_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"USE_YN"	VARCHAR2(10)	DEFAULT 'Y'	NULL
);

COMMENT ON COLUMN "EDOC_SAMPLE"."SAMPLE_NO" IS '일련번호';

COMMENT ON COLUMN "EDOC_SAMPLE"."SAMPLE_DOT_CODE" IS '추후 양식 추가예쩡';

COMMENT ON COLUMN "EDOC_SAMPLE"."SAMPLE_DESC" IS '샘플양식 설명';

COMMENT ON COLUMN "EDOC_SAMPLE"."SAMPLE_FORMAT" IS '사용자서식';

COMMENT ON COLUMN "EDOC_SAMPLE"."EMP_NO" IS '생성자(사원번호)';

COMMENT ON COLUMN "EDOC_SAMPLE"."CREATE_DATE" IS '생성일';

COMMENT ON COLUMN "EDOC_SAMPLE"."MODIFIER" IS '수정자';

COMMENT ON COLUMN "EDOC_SAMPLE"."MODIFT_DATE" IS '수정일';

COMMENT ON COLUMN "EDOC_SAMPLE"."USE_YN" IS '사용유무(Y,N)';

CREATE TABLE "EDOC_APPROVAL" (
	"APRVL_NO"	NUMBER		NOT NULL,
	"APRVL_EDOC_NO"	NUMBER		NULL,
	"APRVL_RANK"	NUMBER		NULL,
	"APRVL_STATUS"	VARCHAR2(10)		NULL,
	"APRVL_COMMENT"	VARCHAR2(4000)		NULL,
	"APRVL_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"EMP_NO"	NUMBER		NULL
);

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_NO" IS '일련번호';

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_EDOC_NO" IS '전자문서번호';

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_RANK" IS '결재순서';

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_STATUS" IS '결재여부(W/Y/N)';

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_COMMENT" IS '결재의견';

COMMENT ON COLUMN "EDOC_APPROVAL"."APRVL_DATE" IS '결재일';

COMMENT ON COLUMN "EDOC_APPROVAL"."EMP_NO" IS '결재자(사원번호)';

CREATE TABLE "PROJECT" (
	"PRO_NO"	NUMBER		NOT NULL,
	"Field"	VARCHAR2(50)		NULL,
	"PRO_TITLE"	VARCHAR2(100)		NULL,
	"START_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"END_DATE"	DATE		NULL,
	"STATUS"	VARCHAR2(10)	DEFAULT '진행중'	NULL,
	"STEP"	VARCHAR2(15)	DEFAULT '분석'	NULL
);

COMMENT ON COLUMN "PROJECT"."PRO_NO" IS '프로젝트 번호';

COMMENT ON COLUMN "PROJECT"."Field" IS '팀장 사번';

COMMENT ON COLUMN "PROJECT"."PRO_TITLE" IS '프로젝트 제목';

COMMENT ON COLUMN "PROJECT"."START_DATE" IS '프로젝트 시작일';

COMMENT ON COLUMN "PROJECT"."END_DATE" IS '프로젝트 종료일';

COMMENT ON COLUMN "PROJECT"."STATUS" IS '진행중/완료';

COMMENT ON COLUMN "PROJECT"."STEP" IS '프로젝트 단계';

CREATE TABLE "CUSTOMER" (
	"CUS_NO"	NUMBER		NOT NULL,
	"PRO_NO"	NUMBER		NULL,
	"CUS_NAME"	VARCHAR2(50)		NULL,
	"MANAGER_NAME"	VARCHAR2(50)		NULL,
	"MANAGER_PHONE"	VARCHAR2(50)		NULL,
	"MANAGER_EMAIL"	VARCHAR2(100)		NULL
);

COMMENT ON COLUMN "CUSTOMER"."CUS_NO" IS '고객사 번호';

COMMENT ON COLUMN "CUSTOMER"."PRO_NO" IS '프로젝트 번호';

COMMENT ON COLUMN "CUSTOMER"."CUS_NAME" IS '고객사 명';

COMMENT ON COLUMN "CUSTOMER"."MANAGER_NAME" IS '고객사 담당자 명';

COMMENT ON COLUMN "CUSTOMER"."MANAGER_PHONE" IS '담당자 연락처';

COMMENT ON COLUMN "CUSTOMER"."MANAGER_EMAIL" IS '담당자 이메일';

CREATE TABLE "TASK" (
	"TASK_NO"	NUMBER		NOT NULL,
	"PRO_NO"	NUMBER		NULL,
	"MEM_NO"	VARCHAR2(50)		NULL,
	"STEP"	VARCHAR2(15)		NULL,
	"TASK_TITLE"	VARCHAR2(100)		NULL,
	"TASK_CONTENT"	VARCHAR2(2000)		NULL,
	"PROGRESS"	NUMBER	DEFAULT 0	NULL,
	"STATUS"	VARCHAR2(10)	DEFAULT '예정'	NULL,
	"END_DATE"	DATE		NULL
);

COMMENT ON COLUMN "TASK"."TASK_NO" IS '업무 번호';

COMMENT ON COLUMN "TASK"."PRO_NO" IS '프로젝트 번호';

COMMENT ON COLUMN "TASK"."STEP" IS '프로젝트 단계';

COMMENT ON COLUMN "TASK"."TASK_TITLE" IS '업무 제목';

COMMENT ON COLUMN "TASK"."TASK_CONTENT" IS '업무 내용';

COMMENT ON COLUMN "TASK"."PROGRESS" IS '업무 진행률';

COMMENT ON COLUMN "TASK"."STATUS" IS '상태';

COMMENT ON COLUMN "TASK"."END_DATE" IS '마감 기한';

CREATE TABLE "MEETING" (
	"MEET_NO"	NUMBER		NOT NULL,
	"PRO_NO"	NUMBER		NULL,
	"MEET_TITLE"	VARCHAR2(100)		NULL,
	"MEET_DATE"	DATE		NULL,
	"DETAIL"	VARCHAR2(500)		NULL,
	"END_YN"	CHAR(1)	DEFAULT 'N'	NULL,
	"SUMMARY"	VARCHAR2(500)		NULL
);

COMMENT ON COLUMN "MEETING"."MEET_NO" IS '회의 번호';

COMMENT ON COLUMN "MEETING"."MEET_TITLE" IS '회의 제목';

COMMENT ON COLUMN "MEETING"."MEET_DATE" IS '회의 일자';

COMMENT ON COLUMN "MEETING"."DETAIL" IS '상세 정보';

COMMENT ON COLUMN "MEETING"."END_YN" IS 'Y/N';

COMMENT ON COLUMN "MEETING"."SUMMARY" IS '종료 후 요약';

CREATE TABLE "RECORD" (
	"RECORD_NO"	NUMBER		NOT NULL,
	"MEET_NO"	NUMBER		NULL,
	"MEM_NO"	VARCHAR2(50)		NULL,
	"RECORD_TITLE"	VARCHAR2(100)		NULL,
	"RECORD_CONTENT"	VARCHAR2(4000)		NULL,
	"UPLOAD_DATE"	DATE	DEFAULT SYSDATE	NULL
);

COMMENT ON COLUMN "RECORD"."RECORD_NO" IS '회의록 번호';

COMMENT ON COLUMN "RECORD"."MEET_NO" IS '회의 번호';

COMMENT ON COLUMN "RECORD"."MEM_NO" IS '작성자 사번';

COMMENT ON COLUMN "RECORD"."RECORD_TITLE" IS '회의록 제목';

COMMENT ON COLUMN "RECORD"."RECORD_CONTENT" IS '회의록 내용';

COMMENT ON COLUMN "RECORD"."UPLOAD_DATE" IS '회의록 등록일';

CREATE TABLE "ATTENDEE" (
	"PRO_NO"	NUMBER		NULL,
	"MEM_NO"	VARCHAR2(50)		NULL,
	"ROLE"	CHAR(7)	DEFAULT '팀원'	NULL
);

COMMENT ON COLUMN "ATTENDEE"."PRO_NO" IS '프로젝트 번호';

COMMENT ON COLUMN "ATTENDEE"."MEM_NO" IS '사원번호';

COMMENT ON COLUMN "ATTENDEE"."ROLE" IS '팀장/팀원';

CREATE TABLE "CALENDAR" (
	"Key"	NUMBER		NOT NULL,
	"PRO_NO"	NUMBER		NULL,
	"CAL_TITLE"	VARCHAR2(100)		NULL,
	"CAL_CONTENT"	VARCHAR2(2000)		NULL,
	"START_DATE"	VARCHAR2(100)		NULL,
	"END_DATE"	VARCHAR2(100)		NULL,
	"COLOR"	VARCHAR2(100)		NULL
);

CREATE TABLE "COMMENT" (
	"COM_NO"	NUMBER		NOT NULL,
	"COM_CONTENT"	VARCHAR2(1000)		NULL,
	"COM_MEM_NO"	VARCHAR2(50)		NULL,
	"COM_BOARD_NO"	NUMBER		NULL,
	"COM_TYPE"	NUMBER	DEFAULT 0	NULL,
	"COM_REGIST_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"COM_MODIFY_DATE"	DATE		NULL,
	"COM_REF"	NUMBER		NULL
);

COMMENT ON COLUMN "COMMENT"."COM_NO" IS '댓글번호';

COMMENT ON COLUMN "COMMENT"."COM_CONTENT" IS '댓글내용';

COMMENT ON COLUMN "COMMENT"."COM_MEM_NO" IS '사원번호';

COMMENT ON COLUMN "COMMENT"."COM_BOARD_NO" IS '게시판번호';

COMMENT ON COLUMN "COMMENT"."COM_TYPE" IS '0:댓글 1:대댓글';

COMMENT ON COLUMN "COMMENT"."COM_REGIST_DATE" IS '댓글작성일';

COMMENT ON COLUMN "COMMENT"."COM_MODIFY_DATE" IS '댓글수정일';

COMMENT ON COLUMN "COMMENT"."COM_REF" IS '참조댓글번호(대댓글용)';

CREATE TABLE "BOARD" (
	"BOARD_NO"	NUMBER		NOT NULL,
	"BOARD_TITLE"	VARCHAR2(300)		NULL,
	"BOARD_CONTENT"	VARCHAR2(3000)		NULL,
	"BOARD_REGIST_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"BOARD_MODIFY_DATE"	DATE		NULL,
	"BOARD_STATUS"	CHAR(3)	DEFAULT 'Y'	NULL,
	"BOARD_TYPE"	CHAR(3)		NULL,
	"BOARD_COUNT"	NUMBER	DEFAULT 0	NULL,
	"BOARD_PINN"	CHAR(3)	DEFAULT 'N'	NULL,
	"BOARD_MEM_NO"	VARCHAR2(50)		NULL
);

COMMENT ON COLUMN "BOARD"."BOARD_NO" IS '게시판번호';

COMMENT ON COLUMN "BOARD"."BOARD_TITLE" IS '게시판제목';

COMMENT ON COLUMN "BOARD"."BOARD_CONTENT" IS '게시판내용';

COMMENT ON COLUMN "BOARD"."BOARD_REGIST_DATE" IS '게시판작성일';

COMMENT ON COLUMN "BOARD"."BOARD_MODIFY_DATE" IS '게시판수정일';

COMMENT ON COLUMN "BOARD"."BOARD_STATUS" IS 'Y:등록상태 N:삭제';

COMMENT ON COLUMN "BOARD"."BOARD_TYPE" IS '공지:N 일반:O 경조사:A';

COMMENT ON COLUMN "BOARD"."BOARD_COUNT" IS '게시판조회수';

COMMENT ON COLUMN "BOARD"."BOARD_PINN" IS '게시판 상단고정여부';

COMMENT ON COLUMN "BOARD"."BOARD_MEM_NO" IS '사원번호';

ALTER TABLE "MEMBER" ADD CONSTRAINT "PK_MEMBER" PRIMARY KEY (
	"MEM_NO"
);

ALTER TABLE "ATTACHMENT" ADD CONSTRAINT "PK_ATTACHMENT" PRIMARY KEY (
	"FILE_NO"
);

ALTER TABLE "CHAT" ADD CONSTRAINT "PK_CHAT" PRIMARY KEY (
	"CHAT_NO"
);

ALTER TABLE "ALERT" ADD CONSTRAINT "PK_ALERT" PRIMARY KEY (
	"ALERT_NO"
);

ALTER TABLE "MESSAGE" ADD CONSTRAINT "PK_MESSAGE" PRIMARY KEY (
	"MSG_NO"
);

ALTER TABLE "EDOC" ADD CONSTRAINT "PK_EDOC" PRIMARY KEY (
	"EDOC_NO"
);

ALTER TABLE "EDOC_SAMPLE" ADD CONSTRAINT "PK_EDOC_SAMPLE" PRIMARY KEY (
	"SAMPLE_NO"
);

ALTER TABLE "EDOC_APPROVAL" ADD CONSTRAINT "PK_EDOC_APPROVAL" PRIMARY KEY (
	"APRVL_NO"
);

ALTER TABLE "PROJECT" ADD CONSTRAINT "PK_PROJECT" PRIMARY KEY (
	"PRO_NO"
);

ALTER TABLE "CUSTOMER" ADD CONSTRAINT "PK_CUSTOMER" PRIMARY KEY (
	"CUS_NO"
);

ALTER TABLE "TASK" ADD CONSTRAINT "PK_TASK" PRIMARY KEY (
	"TASK_NO"
);

ALTER TABLE "MEETING" ADD CONSTRAINT "PK_MEETING" PRIMARY KEY (
	"MEET_NO"
);

ALTER TABLE "RECORD" ADD CONSTRAINT "PK_RECORD" PRIMARY KEY (
	"RECORD_NO"
);

ALTER TABLE "CALENDAR" ADD CONSTRAINT "PK_CALENDAR" PRIMARY KEY (
	"Key"
);

ALTER TABLE "COMMENT" ADD CONSTRAINT "PK_COMMENT" PRIMARY KEY (
	"COM_NO"
);

ALTER TABLE "BOARD" ADD CONSTRAINT "PK_BOARD" PRIMARY KEY (
	"BOARD_NO"
);

